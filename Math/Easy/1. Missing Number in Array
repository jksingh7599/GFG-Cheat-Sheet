#include<bits/stdc++.h>
using namespace std;

int main(){
	//Code Here

    int t;
    cin>>t;

    while(t--){
        int n; 
        cin>>n;

        int arr [n];
        int total = (n * (n+1))/2;

        for(int i = 0; i<n-1; i++)
        {
            cin>>arr[i];
            total = total - arr[i];
        }

        cout<<total<<endl;
    }
	return 0;
}
